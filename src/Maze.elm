module Maze exposing (Feature(..), Room, getRoom)

import Dict exposing (..)
import List exposing (..)


{-
   Orientation is encoded as (0, 1, 2, 3) == (North, East, South, West).

   Rooms are encoded as one character for each of North, East, South, West
     where (W, O, D, S) == (Wall, Open, Door, Secret/Magic Door)
   For example, "OWSW" is read as Open to the North, a Wall to the East, etc.

   Mazes are 32 x 32. The first room, the most North-Western, has coordinates
   (x, y) == (0, 0). The most South-Eastern is (31, 31). The mazes have an
   interesting constraint that in any 2 x 2 square you choose, at least one
   room must be all walls. This guarantees that the maze is all tunnels.
-}


type Feature
    = Solid
    | Open
    | Door
    | MagicDoor
    | Ladder


type alias Room =
    { north : Feature
    , east : Feature
    , south : Feature
    , west : Feature
    , floor : Feature
    , ceiling : Feature
    }


dictLetter2Feature : Dict String Feature
dictLetter2Feature =
    Dict.fromList [ ( "W", Solid ), ( "D", Door ), ( "O", Open ), ( "S", MagicDoor ) ]


getRoom : Int -> Int -> Int -> Room
getRoom lvl x y =
    let
        i =
            (y * 32 + x) * 4 + (1 + y)

        room =
            String.slice i (i + 4) (level lvl).maze

        lookup str pos =
            Maybe.withDefault Solid (Dict.get (String.slice pos (pos + 1) str) dictLetter2Feature)
    in
    { north = lookup room 0
    , east = lookup room 1
    , south = lookup room 2
    , west = lookup room 3
    , floor = getFeature lvl x y (level lvl).floor
    , ceiling = getFeature lvl x y (level lvl).ceiling
    }


level : Int -> { maze : String, floor : List { x : Int, y : Int, feature : Feature }, ceiling : List { x : Int, y : Int, feature : Feature } }
level lvl =
    case lvl of
        0 ->
            { maze = mazeData0, floor = holeData0, ceiling = [] }

        1 ->
            { maze = mazeData1, floor = holeData1, ceiling = holeData0 }

        2 ->
            { maze = mazeData2, floor = [], ceiling = holeData1 }

        3 ->
            { maze = mazeData3, floor = holeData2, ceiling = [] }

        4 ->
            { maze = mazeData4, floor = [], ceiling = holeData2 }

        _ ->
            level 0


getFeature : Int -> Int -> Int -> List { x : Int, y : Int, feature : Feature } -> Feature
getFeature lvl x y features =
    let
        matchPosition e =
            ( e.x, e.y ) == ( x, y )

        feature =
            List.map .feature (List.filter matchPosition features)
    in
    Maybe.withDefault Solid (List.head feature)


holeData0 : List { x : Int, y : Int, feature : Feature }
holeData0 =
